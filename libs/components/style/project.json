{
	"root": "libs/components/style",
	"sourceRoot": "libs/components/style/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nrwl/web:rollup",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/components/style",
				"tsConfig": "libs/components/style/tsconfig.lib.json",
				"project": "libs/components/style/package.json",
				"entryFile": "libs/components/style/src/index.ts",
				"external": ["react/jsx-runtime"],
				"rollupConfig": "@nrwl/react/plugins/bundle-rollup",
				"compiler": "babel",
				"assets": [
					{
						"glob": "libs/components/style/README.md",
						"input": ".",
						"output": "."
					}
				]
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/components/style/**/*.{ts,tsx,js,jsx}"
				]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/components/style"],
			"options": {
				"jestConfig": "libs/components/style/jest.config.js",
				"passWithNoTests": true
			}
		}
	}
}
