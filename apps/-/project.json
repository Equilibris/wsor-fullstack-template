{
  "root": "apps/-",
  "sourceRoot": "apps/-/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/-",
        "index": "apps/-/src/index.html",
        "baseHref": "/",
        "main": "apps/-/src/main.tsx",
        "polyfills": "apps/-/src/polyfills.ts",
        "tsConfig": "apps/-/tsconfig.app.json",
        "assets": ["apps/-/src/favicon.ico", "apps/-/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/-/src/environments/environment.ts",
              "with": "apps/-/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "-:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "-:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/-/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/-"],
      "options": {
        "jestConfig": "apps/-/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
